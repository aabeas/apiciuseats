<div class="container py-5 bg-light">
  <%= render 'shared/page_title', title: "Chef #{@chef.chefname}'s profile" %>
  <div class="row">
    <div class="col-md-1">
    </div>
    <div class="col-md-10">
      <div align="center">
        <p class="center"><%= gravatar_for @chef %></p>
      </div>
      <div align='center'><small>Member since <%= time_ago_in_words(@chef.created_at) %> ago</small></div>
    </div>
    <div class="col-md-1">
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <br>
      <h4 align="center"> ContributionS </h5>
      <%= render 'recipes/recipex', obj: @chef.recipes %>
    </div>
  </div>
</div>
